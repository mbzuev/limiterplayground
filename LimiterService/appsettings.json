{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http.HttpClient": "Warning"
    }
  },
  "AllowedHosts": "*",
  "StorageConcurrencyLimit":
  {
    "MinConcurrency": 1,
    "MaxConcurrency": 50,
    "MaxLatencyMilliseconds": 200,
    "BackoffRatio": 0.5,
    "MaxQueueSize": 0,
    "TargetPercentile": 95,
    "RecalculationRequestCount": 320
  }
}


